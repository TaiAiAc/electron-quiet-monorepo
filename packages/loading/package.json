{
  "name": "@quiteer/electron-loading",
  "version": "0.0.1",
  "description": "基于electron , 提供便捷创建加载窗口的能力",
  "author": "quiteer",
  "license": "MIT",
  "keywords": [
    "electron"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "pages"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "epic-spinners": "^2.0.0"
  }
}

{
  "name": "@quiteer/electronup",
  "version": "0.0.1",
  "description": "基于electron , ",
  "author": "quiteer",
  "license": "MIT",
  "keywords": [
    "vite",
    "tsup",
    "electron-builder",
    "electron"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "electronup": "bin/index.js"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test:dev": "electronup --vite vite.config.ts --tsup tsup.config.ts --port 8090",
    "test:build": "electronup build --vite vite.config.ts --tsup tsup.config.ts --builder builder.config.ts"
  },
  "dependencies": {
    "electron-builder": "^23.6.0",
    "portfinder": "^1.0.32",
    "vite": "^3.2.3"
  },
  "devDependencies": {
    "cac": "^6.7.14",
    "zx": "^7.1.1"
  }
}

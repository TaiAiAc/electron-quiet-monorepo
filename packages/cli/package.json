{
  "name": "@quiteer/electronup",
  "version": "0.0.8",
  "description": "融合构建 electron 应用需要的构建工具,保留原有配置习惯的命令行工具 ",
  "author": "quiteer",
  "license": "MIT",
  "keywords": [
    "vite",
    "tsup",
    "electron-builder",
    "electron"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "electronup": "bin/electronup.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "@types/node": ">= 16",
    "electron": ">= 20",
    "typescript": ">= 4"
  },
  "dependencies": {
    "@swc/core": "^1.3.19",
    "cac": "^6.7.14",
    "electron-builder": "^23.6.0",
    "fs-extra": "^10.1.0",
    "portfinder": "^1.0.32",
    "rimraf": "^3.0.2",
    "ts-swc-register": "github:gityoog/ts-swc-register",
    "tsup": "^6.4.0",
    "vite": "^3.2.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13"
  }
}
